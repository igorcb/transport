<fieldset class="item">
  <%= f.label :product %>
  <%#= collection_select(:f, :product_id, Product.all, :id, :descricao, {include_blank: true}) %>
  <%= f.select( :product_id, Product.all.collect {|p| [ p.descricao, p.id ] }, {include_blank: true} ) %>

  <%= f.label :qtde %>
  <%= f.text_field :qtde %>

  <%= f.label :unidade, "Und. Medida" %>
  <%= f.select( :unidade, select_unidade_medida, {include_blank: true} ) %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>
