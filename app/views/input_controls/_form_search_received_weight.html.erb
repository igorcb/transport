<div class="panel-body">
  <%#= form_tag remote: true, url: received_weight_search_input_controls_path, :method => :get do |f| %>
  <%= form_tag(received_weight_search_input_controls_url, :method => :get, remote: true) do %>
    <div class="form-group">
      <%= label_tag :source_client_group_client_id_eq, "Grupo de Cliente" %>
      <%#= f.collection_select :source_client_group_client_id_eq, GroupClient.order(:descricao), :id, :descricao, {prompt: "SELECIONE O GRUPO DE CLIENTE"}, {class: "form-control"} %>
      <%= select_tag :group_client_id, options_from_collection_for_select(GroupClient.order(:descricao), :id, :descricao, '10'),  class: "form-control" %>
    </div>

    <div class="form-group">
      <%= label_tag :start_date, "Período de " %>
      <%= text_field_tag :start_date, nil, class: "form-control data", placeholder: "Informe a Data ex: 05-12-2014" %>
    </div>
     <div class="form-group">
      <%= label_tag :end_date, "até" %>
      <%= text_field_tag :end_date, nil, class: "form-control data", placeholder: "Informe a Data ex: 05-12-2014" %>
    </div>

    <div class="actions"><%= submit_tag "Search" %></div>
  <% end %>
</div>
