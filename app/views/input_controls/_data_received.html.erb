<div class="list">

  <%= show_line_build @input_control.place_confirmed, "Placa" %>
  <%= show_line_build @input_control.quantity_pallets, "Qtde Paletes Informado" %>
  <%= show_line_build @input_control.team_name, "Equipe" %>

  <%= show_line_build @input_control.hangar, "GalpÃ£o" %>
  <%= show_line_build @input_control.dock, "Doca" %>
  <%= show_line_build @input_control.started_user.email, "Usuario Iniciou" if @input_control.started_user.present? %>
  <%= show_line_build "#{date_br(@input_control.start_time_discharge)} #{time_br(@input_control.start_time_discharge)}", "iniciou a remessa de entrada em (data e hora)" if @input_control.started_user.present? %>
  <%= show_line_build @input_control.user_received.email, "Usuario Terminou" if @input_control.user_received.present? %>
  <%= show_line_build "#{date_br(@input_control.finish_time_discharge)} #{time_br(@input_control.finish_time_discharge)}", "terminou a remessa de entrada em (data e hora)" if @input_control.user_received.present? %>
  <%= show_line_build TimeDifference.between(@input_control.start_time_discharge, @input_control.finish_time_discharge).humanize, "Tempo da descarga" if @input_control.start_time_discharge.present? && @input_control.finish_time_discharge.present? %>
