        <div class="panel-body">
          <%#= search_boardings_path %>
          <%= search_form_for @q, remote: true, url: action_inspectors_path, :method => :get do |f| %>
            <div class="form-group">
              <%#= f.label :id_eq, "No: Embarque " %>
              <%#= f.search_field :id_eq, class: "form-control", placeholder: "" %>  
            </div>
            <div class="form-group">
              <%#= f.label :date_boarding_eq, "Data Embarque " %>
              <%#= f.search_field :date_boarding_eq, class: "form-control data", placeholder: "Informe a Data ex: 05-12-2014" %>  
            </div>
            <div class="form-group">
              <%#= f.label :driver_id_eq, "Motorista" %>
              <%#= f.collection_select :driver_id_eq, Driver.order('nome'), :id, :nome, {include_blank: true}, {class: "form-control"} %>
            </div>
            <div class="form-group">
              <%= label_tag :status, "Status" %>
              <%= select_tag :status, options_for_select(select_status_nfe_dae), prompt: "Selecione o Status", class: "form-control"  %> 
            </div>
            <div class="actions"><%= f.submit "Search" %></div>
          <% end %>
        </div>
