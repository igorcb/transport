<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingEleven">
      <h4 class="panel-title">
        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseEleven" aria-expanded="false" aria-controls="collapseEleven">
          Pesquisar
        </a>
      </h4>
    </div>
    <div id="collapseEleven" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingEleven">
      <div class="panel-body">

      <%#= render "form_search" %>

    </div>
  </div>

</div>

<div class="panel panel-default">
  <div class="panel-heading">Listagem de Ac√µes Fiscais</div>
  <div class="panel-body">

    <%#= link_to 'Novo Cliente', new_client_path, class: "btn btn-primary btn-lg btn-block" %>
    <div class="table-responsive">
      
        <div id="result_client">

        <table class="table table-striped">
          <thead>
            <%= render 'header' %>

          </thead>

          <tbody>
            <%#= render @action_inspectors %>
            <% @action_inspectors.each do |nfe| %>
            <tr>
              <td><%= link_to nfe.ordem_service.input_control.id, nfe.ordem_service.input_control %></td>
              <td><%= link_to nfe.ordem_service.id, nfe.ordem_service %></td>
              <td><%= link_to nfe.ordem_service.client.cpf_cnpj, nfe.ordem_service.client %></td>
              <td><%= link_to nfe.ordem_service.client.nome, nfe.ordem_service.client %></td>
              <td><%= nfe.nfe %></td>
              <td><%= nfe.action_inspector_number %></td>
              <td><%= link_to "Confirm", edit_action_inspector_nfe_key_path(nfe), class: "btn btn-primary btn-sm" %></td>

            </tr>
            <% end %>
          </tbody>

        </table>
      </div>

    </div>    
  </div>
</div>

<script type="text/javascript">
  $(document).ready(function() {
    $('.has_tooltip').tooltip();
  });
</script>
