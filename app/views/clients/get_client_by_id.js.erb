<% if @client.present? %>
  $('#div_result_person').empty(); 
  $('#div_result_person').append("<%= j( render(:partial => 'result_client', locals: {client: @client}) ) %>");
<% else %>
  $('#div_result_person').empty();
  $("#div_result_person").append("<%= j(content_tag(:div, content_tag(:p, "Not found any client"), class: "alert alert-danger")) %> ");
<% end %>
