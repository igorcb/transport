<% if @client.present? %>
  $('#result_client').empty(); 
  $('#result_client').append("<%= j( render(:partial => 'result_client', locals: {client: @client}) ) %>");
<% else %>
  $('#result_client').empty();
  $("#result_client").append("<%= j(content_tag(:div, content_tag(:p, "Not found any client"), class: "alert alert-danger")) %> ");
<% end %>
