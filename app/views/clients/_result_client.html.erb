<% if client.present? %>

	<div class="panel panel-default">
	  <div class="panel-heading">
	    <h2 class="panel-title">Dados do Cliente </h2>
	  </div>
	  <div class="panel-body">

	  <div class="operatings-client-show">
	        
	        <p>
	          <strong>Nome:</strong>
	          <label class="fields"> 
	            <%= link_to "#{ client.nome }", client %> 
	          </label>
	        </p>

	        <p>
	          <strong>CPF/CNPJ:</strong>
	          <label class="fields"><%= client.cpf_cnpj %></label>
	        </p>
	        
	        <p>
	          <strong>Endereco:</strong>
	          <label class="fields"><%= client.endereco %>, <%= client.numero %></label>
	        </p>

	        <p>
	          <strong>Complemento:</strong>
	          <label class="fields"><%= client.complemento %></label>
	        </p>

	        <p>
	          <strong>Bairro:</strong>
	          <label class="fields"><%= client.bairro %></label> - 
	          <strong>Municipio:</strong>
	          <label class="fields"><%= client.cidade %></label>
	        </p>

	  </div>

	</div>



<% end %>



