<% if @employee.present? %>
  $('#div_result_person').empty(); 
  $('#div_result_person').append("<%= j( render(:partial => 'result_employee', locals: {employee: @employee}) ) %>");
<% else %>
  $('#div_result_person').empty();
  $("#div_result_person").append("<%= j(content_tag(:div, content_tag(:p, "Not found any employee"), class: "alert alert-danger")) %> ");
<% end %>
