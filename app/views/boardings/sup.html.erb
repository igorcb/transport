<%= breadcrumbs({"Dashboard": dashboard_oper_path, "Embarques": ""}) %>
<h2>Embarques</h2>
<ul class="list-group dashlist">
  <% @boardings.each do |boarding| %>
    <li class="<%= boarding.status_name.downcase %>">
    <!-- <span class="badge"><%= boarding.qtde_palets %></span> -->
    <%= render 'static_pages/sup_boarding', boarding: boarding %>
    <%= link_to "Informar Equipe", report_team_boarding_path(boarding), {class: "btn btn-info btn-sm"} %>
  </li>
  <% end %>
</ul>
