<% if carrier.present? %>
<!-- <div style="background-color: #F5F5F5;border-radius: 5%; margin-top: 10px;">
	
</div>
 -->
	<div class="panel panel-default">
	  <div class="panel-heading">
	    <h2 class="panel-title">Dados da Transportadora </h2>
	  </div>
	  <div class="panel-body">

	  <div class="operatings-carrier-show">
	        
	        <p>
	          <strong>Nome:</strong>
	          <label class="fields"> 
	            <%= link_to "#{ carrier.nome }", carrier %> 
	          </label>
	        </p>

	        <p>
	          <strong>CPF/CNPJ:</strong>
	          <label class="fields"><%= carrier.cnpj %></label>
	        </p>
	        
	        <p>
	          <strong>Endereco:</strong>
	          <label class="fields"><%= carrier.endereco %>, <%= carrier.numero %></label>
	        </p>

	        <p>
	          <strong>Complemento:</strong>
	          <label class="fields"><%= carrier.complemento %></label>
	        </p>

	        <p>
	          <strong>Bairro:</strong>
	          <label class="fields"><%= carrier.bairro %></label> - 
	          <strong>Municipio:</strong>
	          <label class="fields"><%= carrier.cidade %></label>
	        </p>

	  </div>

	</div>



<% end %>



