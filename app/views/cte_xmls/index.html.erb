<div class="panel panel-default">
  <div class="panel-heading">Listagem de XML de CTE</div>
  <div class="panel-body">
    <%= link_to 'Novo Upload', new_cte_xml_path, class: "btn btn-blue btn-lg btn-block" %>
    <div class="table-responsive">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Chave</th>
            <th>Status</th>
            <th>Option</th>
          </tr>
        </thead>

        <tbody>
          <% @cte_xmls.each do |cte| %>
            <tr>
              <td><%= cte.asset_file_name %></td>
              <td><%= cte.status_name %></td>
              <td><%= cte.error_name %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>    
  </div>
</div>
