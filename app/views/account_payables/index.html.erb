<div class="panel panel-default">
  <div class="panel-heading">Listagem de Contas a Pagar</div>
  <div class="panel-body">
    <%= link_to 'Novo Pagamento', new_account_payable_path, class: "btn btn-primary btn-lg btn-block" %>
    <div class="table-responsive">
      <table class="table table-striped">
        <thead>
          <%= render 'account_header' %>
        </thead>

        <tbody>
          <%= render @account_payables %>
        </tbody>
      </table>
    </div>    
  </div>
</div>
