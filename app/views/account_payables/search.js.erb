<% if @account_payables.present? %>
  $('#result_account_payables').empty(); 
  $("#result_account_payables").append("<%= j(render(:partial => 'search')) %>");
<% else %>
  $('#result_account_payables').empty();
  $("#result_account_payables").append("<%= j(content_tag(:div, content_tag(:p, 'Not found any accounts payable'), class: 'alert alert-danger')) %> ");
<% end %>