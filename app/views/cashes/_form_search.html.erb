
<div class="panel-body">
  <%= search_form_for @q, remote: true, url: cashes_search_path, :method => :get do |f| %>
     <div class="form-group">
      <%= f.label :cash_account_id_eq, "Conta Corrente" %>
      <%= f.collection_select :cash_account_id_eq, CashAccount.order(:nome).collect, :id, :nome, {include_blank: true}, {class: "form-control"} %>
     </div>
     <div class="form-group">
      <%= f.label :data_gteq, "Período de " %>
      <%= f.search_field :data_gteq, class: "form-control data", placeholder: "Informe a Data ex: 05-12-2014" %>  
    </div>
     <div class="form-group">
      <%= f.label :data_lteq, "até" %>
      <%= f.search_field :data_lteq, class: "form-control data", placeholder: "Informe a Data ex: 05-12-2014" %>  
    </div>

    <div class="actions"><%= f.submit "Search" %></div>
  <% end %>
</div>
