<% if @inventory.errors.any? -%>
  $("#inventories_success").hide(300);
  $("#inventories_errors").html("<%= j(render 'shared/error_messages', object: @inventory)%>");
  $("#inventories_errors").show(300);
<% else -%>
  $("#inventories_errors").hide(300);
  $("#inventories_success").append("<div class='alert alert-success'>Successfully created inventory.</div>");
  $(":input:not(input[type=submit])").val("");
  $("#aliases_list").html("<%= j( render "inventories", aliases: @inventories ) %>");
<% end -%>
