<% @nfe_keys.each do |nfe_key| %>
	<tr>
		<td><%= check_box_tag "nfe[ids][#{nfe_key.id}]", nfe_key.id, true, class: "escolha" %></td>
		<td><%= link_to nfe_key.ordem_service.id, nfe_key.ordem_service %></td>
		<td><%= date_br(nfe_key.ordem_service.data_entrega_servico) if nfe_key.ordem_service.data_entrega_servico.present? %></td>
		<td><%= link_to nfe_key.nfe, nfe_key %></td>
		<td><%= nfe_key.chave %></td>
	</tr>
<% end %>
