<fieldset class="item">
  <div class="row">
    <div class="col-xs-2">
      <%= f.text_field :nfe, class: "form-control input-sm", placeholder: "Nfe"  %>
    </div>

    <div class="col-xs-6">
      <%= f.text_field :nfe, maxlength: 44, class: "form-control input-sm", placeholder: "Chave"  %>
    </div>
    <a href="#" class="btn btn-danger btn-sm remove" data-confirm="Are you sure?">[-] Apagar</a>
  </div>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>

