<fieldset class="item">
  <%= f.label :type_service %>
  <%= f.select( :type_service_id, TypeService.all.collect {|p| [ p.descricao, p.id ] }, {include_blank: true} ) %>

  <%= f.label :valor %>
  <%= f.text_field :valor %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>
