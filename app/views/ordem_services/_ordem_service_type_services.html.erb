<fieldset class="item">
  <div class="row">
    <div class="col-xs-2">
      <%= f.select( :type_service_id, TypeService.logistica.collect {|p| [ p.descricao, p.id ] }, {include_blank: true}, {class: "form-control input-sm" } ) %>
    </div>

    <div class="col-xs-2">
      <%= f.text_field :qtde, size: 5, class: "form-control input-sm" %>
    </div>

    <div class="col-xs-2">
      <%= f.text_field :qtde_recebida, size: 5, class: "form-control input-sm" %>
    </div>

    <div class="col-xs-2">
      <%= f.text_field :valor, class: "form-control input-sm money2" %>
    </div>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>
