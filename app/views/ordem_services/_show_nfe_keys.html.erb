
<% if object.nfe_keys.present? %>

  <ul class="list-group">
    <% object.nfe_keys.each do |item| %>
      <li class="list-group-item" >
        <strong> Qtde:</strong> <%= item.qtde %> -
        <strong> NF-e:</strong> <%= link_to item.nfe, item %> -
        <strong>Chave:</strong> <%= link_to item.chave, item %>
        <br />
        <strong> Remessa Ype:</strong> <%= item.remessa_ype %> -
        <strong> Qtde Pallet:</strong> <%= item.qtde_pallet %> -
        <strong> Volume:</strong> <%= number_to_currency( item.volume, unit: "", separator: ",", delimiter: ".", precision: 2) %> -
        <strong> Peso:</strong> <%= number_to_currency( item.peso, unit: "", separator: ",", delimiter: ".", precision: 3)  %> -
        <strong> Frete:</strong> <%= number_to_currency( item.freight_average, unit: "R$ ", separator: ",", delimiter: ".", precision: 4)  %>
        <strong> Image:</strong>
        <% if item.asset.present? %>
          <%= link_to item.asset.url(:original, false) do %>
            <i class="fa fa-check fa-2x" style="color: LimeGreen" aria-hidden="true"></i>
          <% end %>
        <% end %>
        <br />
        <strong> Observacao:</strong>
        <pre><%= item.observation %></pre>

      </li>
    <% end %>
  </ul>
<% else %>
  <h2 class="panel-title"> is not exist nfe association </h2>
<% end %>
