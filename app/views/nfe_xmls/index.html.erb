<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingEleven">
      <h4 class="panel-title">
        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseEleven" aria-expanded="true" aria-controls="collapseEleven">
          Pesquisar
        </a>
      </h4>
    </div>
    <div id="collapseEleven" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingEleven">
      <div class="panel-body">

      <%= render "form_search" %>

    </div>
  </div>

</div>

<div class="panel panel-default">
  <div class="panel-heading">Listagem de NFe XML</div>
  <div class="panel-body">
    <%= link_to 'Novo Upload', new_nfe_xml_path, class: "btn btn-blue btn-lg btn-block" %>

    <div class="table-responsive">
      <div id="result_nfe_xmls">
      <table class="table table-striped">
        <thead>
          <%= render 'header_search' %>
        </thead>

        <tbody>
          <%= render 'list', @nfe_xml %>
        </tbody>
      </table>
      </div>
    </div>
  </div>
</div>





<script type="text/javascript">
  $(document).ready(function() {
    $('.has_tooltip').tooltip();
  });
</script>
