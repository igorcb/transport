<fieldset class="item">

  <%= f.label :setor %>
  <%= f.text_field :setor %>

  <%= f.label :contato %>
  <%= f.text_field :contato %>

  <%= f.check_box :responsavel_carga, checked: f.object.responsavel_carga %> Responsavel pela carga

  <%= f.label :email %>
  <%= f.text_field :email %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
  
</fieldset>
