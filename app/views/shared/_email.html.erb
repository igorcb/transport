<fieldset class="item">
  <div class="row">
    <div class="col-xs-2">
      <%= f.text_field :setor, class: "form-control input-sm", placeholder: "Setor" %>
    </div>

    <div class="col-xs-2">
      <%= f.text_field :contato, class: "form-control input-sm", placeholder: "Contato" %>
    </div>
    
    <div class="col-xs-4">
      <%= f.text_field :email, class: "form-control input-sm", placeholder: "e-mail" %>
    </div>

    <%= f.check_box :responsavel_carga, checked: f.object.responsavel_carga %> Responsavel pela carga

    <a href="#" class="btn btn-danger btn-sm remove" data-confirm="Are you sure?">[-] Apagar</a>
    
    <br/> <br/>
  </div> 

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
  
</fieldset>
