<fieldset class="item">
  <%= f.label :tipo %>
  <%= f.select :tipo, [['FIXO', '0'], ['CELULAR', '1'], ['E-MAIL', '2'], ['TWITTER', '3'], ['FACEBOOK', '4']]  %>

  <%= f.label :nome %>
  <%= f.text_field :nome %>

  <%= f.label :fone %>
  <%= f.text_field :fone %>

  <%= f.label :complemento %>
  <%= f.text_field :complemento %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>
