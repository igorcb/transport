<fieldset class="item">
  <%= f.label :product %>
  <%= collection_select(:f, :product_id, Product.all, :id, :descricao) %>

  <%= f.label :qtde %>
  <%= f.text_field :qtde %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
