<%= form_with(model: street, local: true) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

<div class="row">
  <div class="form_group col-md-6">
    <%= f.label :deposit_id %>
    <%= select("street", "deposit_id", Deposit.all.collect {|p| [ p.name, p.id ] }, { include_blank: "Escolha um depÃ³sito" }, {class: "form-control"}) %>
  </div>
</div>

<div class="row">
  <div class="form_group col-md-6">
    <%= f.label :name %>
    <%= f.text_field :name, id: :street_name, class: "form-control" %>
  </div>
</div>

  <div class="actions">
    <%= f.submit "Salvar", class: "btn btn-primary" %>
    <%= link_to 'Back', streets_path, class: "btn btn-default" %>
  </div>
<% end %>
