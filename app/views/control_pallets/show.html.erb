
<p>
  <strong>Client:</strong>
  <%= @control_pallet.client.nome %>
</p>

<p>
  <strong>Saldo de Palletes para este client:</strong>
  <%= ControlPallet.saldo(@control_pallet.client.id) %>
</p>

<p>
  <strong>Data:</strong>
  <%= date_br @control_pallet.data %>
</p>

<p>
  <strong>Transportador:</strong>
  <%= @control_pallet.carrier.nome if @control_pallet.carrier.present? %>
</p>

<p>
  <strong>Qte:</strong>
  <%= @control_pallet.qte %>
</p>

<p>
  <strong>Tipo:</strong>
  <%= @control_pallet.credito_debito %>
</p>

<p>
  <strong>Historico:</strong>
  <%= @control_pallet.historico %>
</p>

<p>
  <strong>Nfe:</strong>
  <%= @control_pallet.nfe %>
</p>

<p>
  <strong>Nfe Original:</strong>
  <%= @control_pallet.nfe_original %>
</p>

<p>
  <strong>Nfd:</strong>
  <%= @control_pallet.nfd %>
</p>


<p>
  <strong>NFD Original:</strong>
  <%= @control_pallet.nfd_original %>
</p>


<div class="form-group">
  <%= link_to 'Editar', edit_control_pallet_path(@control_pallet), class: "btn btn-primary" %> 
  <%= link_to 'Voltar', control_pallets_path, class: "btn btn-default" %>
</div>
