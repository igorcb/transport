<% if @ordem_service_type_services.present? %>
  $('#results').empty(); 
  $("#results").append("<%= j(render(:partial => "search")) %>");
<% else %>
  $('#results').empty();
  $("#results").append("<%= j(content_tag(:div, content_tag(:p, "Not found any ordem service type service"), class: "alert alert-danger")) %> ");
<% end %>
