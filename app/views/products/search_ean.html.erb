<div class="row">
  <form id="search" action="" method="get">
    <div class="form-group col-md-5 has-feedback">
      <label for="qtde">Ler codigo de barras</label>
      <div class="input-group">
        <a href="http://zxing.appspot.com/scan?ret=<%= search_ean_products_path %>?ean={CODE}" class="input-group-addon">
          <i class="fas fa-barcode"></i>
        </a>
        <input type="text" id="barcode" name="ean" class="form-control" value="<%= params["ean"] %>">
        <a href="#" onClick="document.getElementById('search').submit();"botton class="btn btn-primary input-group-addon"><i class="fas fa-search"></i></a>
      </div>
    </div>
  </form>
</div>

<div class="">

  <% @products.each do |product| %>

    <div class="list">
      <%= show_line_build product.descricao, "Produto" %>
      <%= show_line_build product.cod_prod, "Cod. Prod." %>
      <%= show_line_build product.ean, "EAN" %>
      <%= show_line_build product.ean_trib, "EAN Tributado" %>

      <%= show_line_build product.unid_medida, "Unid. Medida" %>
      <%= show_line_build product.weight_liquid, "Peso Liquido" %>
      <%= show_line_build product.weight_gross, "Peso Bruto" %>
      <%= show_line_build product.width, "Largura"  %>
      <%= show_line_build product.length, "Comprimento"  %>
      <%= show_line_build product.height, "Altura"  %>

      <%= show_line_build product.ballast, "Latro do Pallet"  %>
      <%= show_line_build product.layer_pallet, "Camada do Pallet"  %>
      <%= show_line_build product.total_box_pallet, "Caixas por Pallet"  %>
      <%= show_line_build product.height_maximum_pallet, "Altura MÃ¡xima Pallet"  %>
    </div>

    <div class="form-group">
      <%= link_to 'Editar', edit_product_path(product), class: "btn btn-blue btn" %>
    </div>

  <% end %>
